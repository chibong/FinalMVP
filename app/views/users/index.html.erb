<h1>Users</h1>

<p><%= link_to 'Add a New User', new_user_url %></p>

<table class="table table-hover table-bordered table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
    </tr>
  </thead>

  <tbody>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= link_to "Give Feedback", questions_url(user) %> </td>
    </tr>
  <% end %>
  </tbody>
</table>

